<!DOCTYPE html>
<html lang="en">
<head>
  <title>TinyApp</title>
</head>
<body>
  <% include partials/_header %>
    <!-- Add your EJS code here -->
  <ul>
    <% Object.keys(urls).forEach(function (key) { %>
     <li><%= key %> : <%= urls[key] %></li>
     <% const urlID = "/urls/" + key + "/delete/" %>
     <form method="POST" action= <%= urlID %> >
      <button>Delete</button>
     </form>
    <% }) %>
  </ul>
  <p><a href="/urls/new">Make new one</a></p>
</body>
</html>